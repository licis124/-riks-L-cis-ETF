#include <stdio.h>

int main()
{
    long a;
    long b;
 
    printf("Cienijamais lietotajs, ludzu ievadi pirmo skaitli 400 000 :");
scanf("%ld",&a); 
    printf("Cienijamais lietotajs, ludzu ievadi otro skaitli 100 000 :");
scanf("%ld",&b);
long c= a*b;
     printf("400 000 * 100 000 :%ld",c);
    return 0;
}
